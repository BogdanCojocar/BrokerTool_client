<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>

<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
	fx:controller="com.brokerTool.gui.BrokerToolController">
	<children>
		<Pane prefHeight="341.0" prefWidth="485.0000999999975" fx:id="brokerPane">
			<children>
				<TableView fx:id="brokerTable" layoutX="14.0" layoutY="57.0"
					onMouseClicked="#handleMouseClick" prefHeight="229.0" prefWidth="456.999977929685">
					<columns>
						<TableColumn maxWidth="5000.0" minWidth="10.0"
							prefWidth="43.0" text="Id" fx:id="broker_id" />
						<TableColumn maxWidth="5000.0" minWidth="10.0"
							prefWidth="225.0" text="Name" fx:id="name" />
						<TableColumn maxWidth="5000.0" minWidth="10.0"
							prefWidth="186.0" text="Address" fx:id="address" />
					</columns>
				</TableView>
				<Label layoutX="14.0" layoutY="14.0" prefHeight="29.9609375"
					prefWidth="101.0" text="Broker table">
					<font>
						<Font name="System Bold" size="14.0" />
					</font>
				</Label>
				<Button layoutX="359.0" layoutY="297.0" mnemonicParsing="false"
					onAction="#showCustomers" prefHeight="29.9609375" prefWidth="113.0"
					text="View customers">
					<font>
						<Font name="System Bold" size="12.0" fx:id="x1" />
					</font>
				</Button>
				<Button font="$x1" layoutX="14.0" layoutY="297.0" onAction="#showStocks"
					mnemonicParsing="false" prefHeight="29.9609375" prefWidth="87.0"
					text="View stocks" />
			</children>
		</Pane>
	</children>
</VBox>
